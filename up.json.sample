{
  "name": "advance-form",
  "profile": "default",
  "regions": [
    "ca-central-1"
  ],
  "proxy": {
    "command": "FLASK_APP=app.py python -m flask run -p $PORT"
  },
  "static": {
    "dir": "static",
    "prefix": "/static"
  },
  "stages": {
    "production": {
      "domain": "PROD_DOMAIN"
    },
    "staging": {
      "domain": "DEV_DOMAIN"
    }
  }
}



